{
    "name": "isolate/symfony-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Isolate Symfony Bundle",
    "keywords": ["isolate", "symfony", "bundle"],
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "autoload": {
        "psr-0": {
          "": "src",
          "Isolate\\Symfony\\IsolateBundle\\Tests": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "symfony/http-kernel": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "doctrine/collections": "1.*",
        "isolate/unit-of-work": "1.0.*@dev",
        "isolate/lazy-objects": "1.0.*@dev",
        "isolate/isolate": "1.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "symfony/framework-bundle": "~2.3",
        "symfony/finder": "~2.3",
        "ocramius/proxy-manager": "~1.0",
        "symfony/proxy-manager-bridge": "~2.3",
        "symfony/yaml": "~2.3",
        "isolate/doctrine-bridge": "1.0.*@dev",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/orm" : "~2.2,>=2.2.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
